<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Teachers</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Id</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% rows.forEach(data => { %>
          <tr>
            <td><%= data.id %></td>
            <td><%= data.first_name %></td>
            <td><%= data.last_name %></td>
            <td><%= data.email %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>